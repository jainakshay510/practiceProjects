
public class CountFrequency {
	
	public static void main(String[] args) {
		
		
		
	}
	
	
	public void countFrequency(String s) {
		
		char[] ch=s.toCharArray();
		
		int[] freq=new int[26];
		
		for(int i=0;i<s.length();i++) {
			freq[s.charAt(i)-'a']+=1;
			System.out.println(freq[s.charAt(i)-'a']);
		}
		
		
		
	}

}
